grammar edu.unq.objetos3.PlanificacionConferencia with org.eclipse.xtext.common.Terminals

generate planificacionConferencia "http://www.unq.edu/objetos3/PlanificacionConferencia"

Model:
	charlas+=Charla*;
	
Charla:
	'Charla' titulo=STRING 'por' (oradores+=Orador)+ 'durante' duracion=INT
;

Orador:
	nombre=STRING 'de' organizacion=STRING
;
